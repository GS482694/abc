package third;
 
public class ddd {
	public static void main(String[]args){
		java.util.Scanner input=new java.util.Scanner(System.in);
		int[] list=new int[200];
		System.out.print("Enter list1: ");
		int[] str1=new int[100];
		str1[0]=input.nextInt();
		for(int i=1;i<=str1[0];i++) {
			str1[i]=input.nextInt();
		}
		System.out.print("Enter list2: ");
		int[] str2=new int[100];
		str2[0]=input.nextInt();
		for(int i=1;i<=str2[0];i++) {
			str2[i]=input.nextInt();
		}
		list=merge(str1,str2);
		for(int i=0;i<str1[0]+str2[0];i++)
        {
            for(int j=0;j<str1[0]+str2[0]-1-i;j++)
            {
                if((list[j]>list[j+1]))
                {
                    int temp=list[j];
                    list[j]=list[j+1];
                    list[j+1]=temp;
                }
            }
        }
		System.out.print("The merged list is ");
        for(int i=0;i<str1[0]+str2[0];i++)
            System.out.print(list[i]+" ");
	}
	
	public static int[] merge(int[] list1,int[] list2) {
		int[] list=new int[200];
		int a=0;
			for(int j=1;j<=list1[0];j++) {
				list[a]=list1[j];
				a++;
			}
			for(int j=1;j<=list2[0];j++) {
				list[a]=list2[j];
				a++;
			}
		
		return list;
	}
}
